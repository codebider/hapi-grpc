{
  "name": "glints",
  "version": "0.0.0",
  "description": "Is it open",
  "main": "src/app.js",
  "scripts": {
    "clean": "rm -rf logs && npm run install:clean && npm run test:clean",
    "debug": "nodemon --inspect src/app.js",
    "develop": "NODE_ENV=local nodemon src/app.js",
    "eslint": "eslint . --ignore-path .gitignore",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier --write 'src/**/*.json'",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier",
    "start": "node src/app.js --name \"Is it Open\"",
    "sequelize-cli": "NODE_ENV=local npx sequelize-cli",
    "test": "jest --maxWorkers=4",
    "test:ci": "npm run test -- --ci --bail",
    "test:clean": "rm -rf coverage",
    "test:coverage": "npm run test -- --coverage",
    "test:updateSnapshot": "npm run test -- --updateSnapshot"
  },
  "keywords": [
    "glints",
    "Isitopen"
  ],
  "author": "Daniel Le",
  "license": "ISC",
  "dependencies": {
    "@hapi/hapi": "18.4.0",
    "@hapi/inert": "5.2.2",
    "@hapi/joi": "16.1.8",
    "@hapi/vision": "5.5.4",
    "awilix": "4.2.3",
    "bcrypt": "3.0.8",
    "es6-error": "4.1.1",
    "hapi-swagger": "11.1.0",
    "is_js": "0.9.0",
    "json-stringify-safe": "5.0.1",
    "jsonwebtoken": "8.5.1",
    "kind-of": "6.0.3",
    "lodash": "4.17.15",
    "nconf": "0.10.0",
    "pg": "7.18.1",
    "pg-hstore": "2.3.3",
    "sequelize": "5.21.4",
    "socket.io": "2.3.0"
  },
  "devDependencies": {
    "eslint": "5.15.3",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-eslint-comments": "3.1.1",
    "eslint-plugin-hapi": "4.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.4.1",
    "eslint-plugin-jsdoc": "4.6.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-optimize-regex": "1.1.7",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-unicorn": "8.0.1",
    "jest": "25.1.0",
    "jest-extended": "0.11.5",
    "jest-plugin-unhandled-promise": "2.9.0",
    "jest-when": "2.7.0",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "sequelize-cli": "5.5.1"
  },
  "engines": {
    "node": ">=10.13"
  }
}
